void reverseString(char* s, int sSize){  //string pointer and size is available
   //In For loop i checks for 0th index and j checks last index in reverse order
   for(int i=0, j = sSize -1; i < j; i++, j--){
       //Swapping
       char temp = s[i];  
       s[i] = s[j];
       s[j] = temp;
   }
}
//Time Complexity: O(n)
